 => {\r\n  switch (action.type) {\r\n    case GET_PRODUCTS :\r\n      return {\r\n        ...state,\r\n        products: action.product\r\n      }\r\n    case GET_PRODUCTS_LOCAL :\r\n      return {\r\n        ...state,\r\n        productsLocal: action.productsLocal\r\n      }\r\n  }\r\n  return state\r\n\r\n}\r\n\r\nexport default productReducer\r\n","C:\\Entrepreunariat\\Projets\\MaxEtLea\\maxandlea\\frontend\\src\\store\\actions\\product.js",[],"C:\\Entrepreunariat\\Projets\\MaxEtLea\\maxandlea\\frontend\\src\\context\\AppContext.js",["186"],"import React, {useState, useEffect} from 'react';\r\n\r\nexport const AppContext = React.createContext([\r\n  {},\r\n  newCart => {}\r\n]);\r\n\r\nexport const AppProvider = props => {\r\n  const [cart, setCart] = useState(null);\r\n  useEffect(() => {\r\n    if ( process.browser) {\r\n      let cartData = localStorage.getItem('woo-next-cart');\r\n      cartData = null !== cartData ? JSON.parse(cartData) : ''\r\n      setCart(cartData)\r\n      console.log(cart)\r\n    }\r\n  }, []);\r\n  return (\r\n    <AppContext.Provider value={[ cart, setCart]}>\r\n      {props.children}\r\n    </AppContext.Provider>\r\n  )\r\n}\r\n","C:\\Entrepreunariat\\Projets\\MaxEtLea\\maxandlea\\frontend\\src\\screens\\CartScreen.js",["187","188","189","190","191","192","193"],"C:\\Entrepreunariat\\Projets\\MaxEtLea\\maxandlea\\frontend\\src\\components\\CartItem.js",["194","195","196","197","198"],"import React, {useContext, useEffect, useState} from 'react';\r\nimport {useDispatch, useSelector} from \"react-redux\";\r\nimport * as productAction from \"../store/actions/product\";\r\nimport {AppContext} from \"../context/AppContext\";\r\nimport {useHistory} from \"react-router-dom\";\r\n\r\n\r\nconst CartItem = ({item}) => {\r\n\r\n\r\n\r\n  const [cart, setCart] = useContext(AppContext)\r\n\r\n  let router = useHistory();\r\n\r\n  const [viewCart, setViewCart] = useState(false);\r\n\r\n\r\n  const dispatch = useDispatch();\r\n  const product = use